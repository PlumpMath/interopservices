<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <VersionPrefix>0.1.0</VersionPrefix>
    <FileVersion>$(VersionPrefix)</FileVersion>
    <TargetFramework>netstandard1.3</TargetFramework>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <AssemblyName>THNETII.InteropServices.SafeHandles</AssemblyName>
    <PackageId>THNETII.InteropServices.SafeHandles</PackageId>
    <NetStandardImplicitPackageVersion>1.6.1</NetStandardImplicitPackageVersion>
    <PackageTargetFallback>$(PackageTargetFallback);dnxcore50;dotnet5.4;portable-net46+win10</PackageTargetFallback>
  </PropertyGroup>
 
  <Import Project="..\..\common.props" />

  <PropertyGroup Condition=" '$(TargetFramework)' == 'netstandard1.3' ">
    <DebugType>portable</DebugType>
  </PropertyGroup>

</Project>
